<UserControl
    x:Class="WREdit.Views.ProcessorPropertiesView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WREdit.Views"
    xmlns:u="clr-namespace:WREdit.Utils"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <UserControl.Resources>
        <u:PropertyTemplateSelector x:Key="PropertyTemplateSelector"/>
    </UserControl.Resources>
    <Grid>
        <ItemsControl Margin="4" ItemsSource="{Binding Properties}" ItemTemplateSelector="{StaticResource PropertyTemplateSelector}"/>
    </Grid>
</UserControl>
